<!DOCTYPE html>
<html>
<head>
 <%= render 'layouts/lenticular_cms/head' %>
 <%= tinymce_assets %>
</head>
<body id="lcms-db">
  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-12" id="lcms-db-header">
        <h1><%= link_to lenticular_cms.dashboard_path do %><span class="glyphicon glyphicon-home"></span> Lenticular CMS<% end %></h1>
      </div>
    </div>
  </div>
  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-2" id="lcms-db-sidebar">
        <%= render 'layouts/lenticular_cms/admin_sidebar' %>
      </div>
      <div class="col-sm-10" id="lcms-db-content">
        <div class="row">
          <div class="col-sm-12" id="lcms-db-messages">
            <%= flash_messages %>
          </div>
        </div>
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>